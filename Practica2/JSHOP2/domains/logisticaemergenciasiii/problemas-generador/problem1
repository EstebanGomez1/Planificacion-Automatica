		(Transportador t1)
		(loc-transportador t1 deposito)
		(capacidad-max-transportador t1 10)
		(capacidad-actual-transportador t1 0)
		(contenido-transportador t1 comida 0)
		(contenido-transportador t1 medicina 0)
		(loc-necesita-contenido Loc2 comida 39)
		(loc-necesita-contenido Loc2 medicina 11)
		(loc-necesita Loc2 50)
		(cantidad-cajas comida 49)
		(cantidad-cajas medicina 21)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
	)
	(;Task to solve
		(enviar-todo)
	)
)
