(defproblem problem logisticaemergenciasiii 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(LOC Loc2)
		(CONTENIDO comida)
		(CONTENIDO medicina)
		(BRAZO brazo1)
		(loc-dron dron1 deposito)

		(Transportador t1)
		(loc-transportador t1 deposito)
		(capacidad-max-transportador t1 150)
		(capacidad-actual-transportador t1 0)
		(contenido-transportador t1 comida 0)
		(contenido-transportador t1 medicina 0)

		(loc-necesita-contenido Loc1 comida 14)
		(loc-necesita-contenido Loc1 medicina 36)
		(loc-necesita Loc1 50)
		(loc-necesita-contenido Loc2 comida 16)
		(loc-necesita-contenido Loc2 medicina 84)
		(loc-necesita Loc2 100)
		(cantidad-cajas comida 40)
		(cantidad-cajas medicina 130)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
	)
	(;Task to solve
		(enviar-todo)
	)
)