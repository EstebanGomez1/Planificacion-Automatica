		(loc-cajas deposito)
		(loc-dron dron1 deposito)
		(brazo-dron-free dron1 brazo1)
		(loc-necesita-contenido b comida 2)
		(loc-necesita-contenido b medicina 8)
		(loc-necesita-contenido a comida 3)
		(loc-necesita a 3)
		(loc-necesita b 10)

		(coste-viaje 0)


	)(;Task to solve
		(enviar-todo)
	)
  )
