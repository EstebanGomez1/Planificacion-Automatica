(defproblem problem logisticaemergencias 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(LOC Loc2)
		(LOC Loc3)
		(CAJA caja1)
		(CAJA caja2)
		(CONTENIDO food)
		(CONTENIDO medicine)
		(PERSONA persona0)
		(PERSONA persona1)
		(BRAZO brazo1)
		(BRAZO brazo2)
		
		(loc-dron dron1 deposito)
		(persona-necesita-contenido persona0 food)
		(persona-necesita-contenido persona0 medicine)
		(loc-caja caja2 deposito)
		(loc-caja caja1 deposito)
		(loc-persona persona1 Loc1)
		(caja-free caja1)
		(caja-free caja2)
		(loc-persona persona0 Loc1)
		(caja-contenido caja2 medicine)
		(caja-contenido caja1 food)
		(brazo-dron-free dron1 brazo1)
		(brazo-dron-free dron1 brazo2)
	)
	(;Task to solve
		(enviar-todo)
	)
)
