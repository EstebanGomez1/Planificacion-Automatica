(defproblem problem2 logisticaemergencias 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(LOC Loc2)
		(LOC Loc3)
		(LOC Loc4)
		(LOC Loc5)
		(LOC Loc6)
		(CAJA caja1)
		(CAJA caja2)
		(CAJA caja3)
		(CAJA caja4)
		(CONTENIDO food)
		(CONTENIDO medicine)
		(PERSONA persona0)
		(PERSONA persona1)
		(PERSONA persona2)
		(PERSONA persona3)
		(BRAZO brazo1)
		(BRAZO brazo2)
		(loc-dron dron1 deposito)
		(persona-necesita-contenido persona0 food)
		(loc-persona persona0 Loc3)
		(caja-contenido caja1 food)
		(loc-caja caja1 deposito)
		(caja-free caja1)
		(persona-necesita-contenido persona1 food)
		(loc-persona persona1 Loc5)
		(caja-contenido caja2 food)
		(loc-caja caja2 deposito)
		(caja-free caja2)
		(persona-necesita-contenido persona1 medicine)
		(loc-persona persona1 Loc2)
		(caja-contenido caja3 medicine)
		(loc-caja caja3 deposito)
		(caja-free caja3)
		(persona-necesita-contenido persona2 food)
		(loc-persona persona2 Loc2)
		(caja-contenido caja4 food)
		(loc-caja caja4 deposito)
		(caja-free caja4)
		(brazo-dron-free dron1 brazo1)
		(brazo-dron-free dron1 brazo2)
	)
	(;Task to solve
		(enviar-todo)
	)
)