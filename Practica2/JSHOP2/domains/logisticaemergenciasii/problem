(defproblem problem logisticaemergenciasii
	(;Initial state
		(DRON dron1) (LOC a) (LOC b) (LOC c) (LOC d) (LOC deposito)
		(BRAZO brazo1)
		(TRANSPORTADOR t1)
		(cantidad-cajas medicina 10)
		(cantidad-cajas comida 10)
		(loc-cajas deposito)
		(loc-dron dron1 deposito)
		(brazo-dron-free dron1 brazo1)
		(loc-necesita-contenido a comida 5)
		(capacidad-max-transportador t1 8)
		(capacidad-actual-transportador t1 0)
		(coste-viaje 0)

	)(;Task to solve
		(enviar-todo)
	)
  )
