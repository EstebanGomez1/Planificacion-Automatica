(defproblem problem logisticaemergenciasii 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(LOC Loc2)
		(CONTENIDO comida)
		(CONTENIDO medicina)
		(BRAZO brazo1)
		(loc-dron dron1 deposito)

		(Transportador t2)
		(loc-transportador t2 deposito)
		(capacidad-max-transportador t2 50)
		(capacidad-actual-transportador t2 0)
		(contenido-transportador t2 comida 0)
		(contenido-transportador t2 medicina 0)

		(Transportador t3)
		(loc-transportador t3 deposito)
		(capacidad-max-transportador t3 100)
		(capacidad-actual-transportador t3 0)
		(contenido-transportador t3 comida 0)
		(contenido-transportador t3 medicina 0)
		(loc-necesita-contenido Loc2 comida 30)
		(loc-necesita-contenido Loc2 medicina 70)
		(loc-necesita-contenido Loc1 comida 20)
		(loc-necesita-contenido Loc1 medicina 30)
		(loc-necesita Loc1 50)
		(loc-necesita Loc2 100)
		(cantidad-cajas comida 200)
		(cantidad-cajas medicina 200)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
	)
	(;Task to solve
		(enviar-todo)
	)
)
