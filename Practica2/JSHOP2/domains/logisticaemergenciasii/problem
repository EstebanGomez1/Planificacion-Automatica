(defproblem problem logisticaemergenciasii
	(;Initial state
		(DRON dron1) (LOC a) (LOC b) (LOC c) (LOC d) (LOC deposito)
		(BRAZO brazo1)
		(TRANSPORTADOR t1)
		(CONTENIDO medicina)
		(CONTENIDO comida)
		(cantidad-cajas medicina 2)
		(cantidad-cajas comida 4)
		(loc-cajas deposito)
		(loc-dron dron1 deposito)
		(loc-transportador t1 deposito)
		(brazo-dron-free dron1 brazo1)
		(loc-necesita-contenido a comida 3)
		(capacidad-max-transportador t1 5)
		(capacidad-actual-transportador t1 0)
		(coste-viaje 0)
		(contenido-transportador t1 comida 0)
		(contenido-transportador t1 medicina 0)

	)(;Task to solve
		(enviar-todo)
	)
  )
