(defproblem problem1 logisticaemergenciasii 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(LOC Loc2)
		(Transportador transportador1)
		(CONTENIDO comida)
		(CONTENIDO medicina)
		(BRAZO brazo1)
		(loc-dron dron1 deposito)
		(loc-transportador transportador1 deposito)
		(capacidad-max-transportador transportador1 2)
		(capacidad-actual-transportador transportador1 0)
		(contenido-transportador transportador1 comida 0)
		(contenido-transportador transportador1 medicina 0)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
		(loc-necesita-contenido Loc1 medicina 4)
		(loc-necesita-contenido Loc1 comida 2)
		(loc-necesita Loc1 6)
	)
	(;Task to solve
		(enviar-todo)
	)
)