		(loc-dron dron1 deposito)

		(Transportador t1)
		(loc-transportador t1 deposito)
		(capacidad-max-transportador t1 20)
		(capacidad-actual-transportador t1 0)
		(contenido-transportador t1 comida 0)
		(contenido-transportador t1 medicina 0)

		(Transportador t2)
		(loc-transportador t2 deposito)
		(capacidad-max-transportador t2 50)
		(capacidad-actual-transportador t2 0)
		(contenido-transportador t2 comida 0)
		(contenido-transportador t2 medicina 0)

		(Transportador t3)
		(loc-transportador t3 deposito)
		(capacidad-max-transportador t3 100)
		(capacidad-actual-transportador t3 0)
		(contenido-transportador t3 comida 0)
		(contenido-transportador t3 medicina 0)
		(loc-necesita-contenido Loc3 comida 12)
		(loc-necesita-contenido Loc3 medicina 4)
		(loc-necesita-contenido Loc1 comida 6)
		(loc-necesita-contenido Loc1 medicina 10)
		(loc-necesita-contenido Loc3 comida 14)
		(loc-necesita-contenido Loc3 medicina 2)
		(loc-necesita Loc1 16)
		(loc-necesita Loc3 32)
		(cantidad-cajas comida 42)
		(cantidad-cajas medicina 26)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
	)
	(;Task to solve
		(enviar-todo)
	)
)