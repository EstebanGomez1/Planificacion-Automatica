(defproblem problem logisticaemergenciasii 
	(;Initial state
		(DRON dron1)
		(LOC deposito)
		(LOC Loc1)
		(CONTENIDO comida)
		(CONTENIDO medicina)
		(BRAZO brazo1)
		(loc-dron dron1 deposito)

		(Transportador t1)
		(loc-transportador t1 deposito)
		(capacidad-max-transportador t1 5)
		(capacidad-actual-transportador t1 0)
		(contenido-transportador t1 comida 0)
		(contenido-transportador t1 medicina 0)
		(loc-necesita-contenido Loc2 comida 7)
		(loc-necesita-contenido Loc2 medicina 3)
		(loc-necesita Loc2 10)
		(cantidad-cajas comida 17)
		(cantidad-cajas medicina 13)
		(brazo-dron-free dron1 brazo1)
		(loc-cajas deposito)
		(coste-viaje 0)
	)
	(;Task to solve
		(enviar-todo)
	)
)